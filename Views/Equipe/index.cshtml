@model Equipe
@{
    ViewData["Title"] = "Equipes";
}

<form method = "POST" action = '@Url.Action("Cadastrar")'>
    <label>ID</label>
    <input type="text" name="IdEquipe"/>

    <label>Nome</label>
    <input type="text" name="Nome"/>

    <label>Imagem</label>
    <input type="text" name="Imagem"/>

    <button type="submit">Cadastrar</button>
</form>

<table class="table table-striped table-respinsive">
    <thead>
        <th>ID</th>
        <th>Nome</th>
        <th>Imagem</th>
    </thead>
    <tbody>
        @foreach (Equipe equipe in ViewBag.Equipes){
           <tr>
               <td>@equipe.IdEquipe</td>
               <td>@equipe.Nome</td>
               <td>@equipe.Imagem</td>
           </tr> 
        }
    </tbody>
</table>